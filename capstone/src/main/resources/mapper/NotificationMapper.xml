<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.usyd.capstone.mapper.NotificationMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.usyd.capstone.entity.Notification">
        <id column="id" property="id" />
        <result column="content" property="content" />
        <result column="receivedUserId" property="receivedUserId" />
        <result column="sendTime" property="sendTime" />
        <result column="isRead" property="isRead" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, content, receivedUserId, sendTime, isRead
    </sql>

</mapper>
